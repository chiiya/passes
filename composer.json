{
    "name": "chiiya/passes",
    "description": "PHP library for creating iOS and Android Wallet Passes",
    "keywords": [
        "chiiya",
        "passes",
        "wallet",
        "apple",
        "google",
        "android",
        "ios"
    ],
    "homepage": "https://github.com/chiiya/passes",
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Witte",
            "email": "github@chiiya.moe",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "ext-zip": "*",
        "antwerpes/data-transfer-object": "^1.0",
        "google/auth": "^1.18",
        "spatie/data-transfer-object": "^3.7",
        "symfony/validator": "^7.1"
    },
    "require-dev": {
        "chiiya/code-style-php": "^3.0",
        "phpro/grumphp": "^2.7",
        "phpunit/phpunit": "^11.3",
        "symfony/var-dumper": "^7.1",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Chiiya\\Passes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chiiya\\Passes\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
